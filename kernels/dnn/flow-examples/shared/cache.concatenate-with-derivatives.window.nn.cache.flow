<network>
  <out name="features"/>

  <param name="id"/>

  <param name="input-file"/>
  <param name="start-time"/>
  <param name="end-time"/>
  <param name="track"/>

  <param name="speaker"/>

  <node name="base-feature-extraction-cache" filter="generic-cache" 
	id="$(id)"/>

  <node name="concatenate-with-derivatives" filter="concatenate-with-derivatives.flow"/>
  <link from="base-feature-extraction-cache" to="concatenate-with-derivatives:in"/>

  <node name="window"
      filter="signal-vector-f32-sequence-concatenation" max-size="9"
      right="4" margin-condition="present-not-empty"
      expand-timestamp="false"/>
  <link from="concatenate-with-derivatives:out" to="window"/>

  <node name="nn" filter="neural-network-forward" id="$(id)" />
  <link from="window" to="nn"/>

  <node name="nn-cache" filter="generic-cache" id="$(id)"/>
  <link from="nn" to="nn-cache"/>
  <link from="nn-cache" to="network:features"/>
</network>
