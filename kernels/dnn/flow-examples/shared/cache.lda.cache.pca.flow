<network>
  <out name="features"/>

  <param name="id"/>

  <param name="input-file"/>
  <param name="start-time"/>
  <param name="end-time"/>
  <param name="track"/>

  <param name="speaker"/>

  <node name="base-feature-extraction-cache" filter="generic-cache" 
	id="$(id)"/>

  <node name="lda" filter="lda.flow"/>
  <link from="base-feature-extraction-cache" to="lda:in"/>

  <node name="nn-cache" filter="generic-cache" id="$(id)"/>
  <node name="pca" filter="signal-matrix-multiplication-f32"/>
  <link from="nn-cache" to="pca" />

<!--
  Sync. feature caches
-->
  <node name="feature-synchronization"
	filter="signal-repeating-frame-prediction"/>
  <link from="lda:out" to="feature-synchronization:target"/>
  <link from="pca" to="feature-synchronization"/>

  <node name="concatenation" 
        filter="generic-vector-f32-concat"
        check-same-length="true"
        timestamp-port="feature-1"/>
  <link from="lda:out" to="concatenation:feature-1"/>
  <link from="feature-synchronization" to="concatenation:feature-2"/>


  <link from="concatenation" to="network:features"/>
</network>
