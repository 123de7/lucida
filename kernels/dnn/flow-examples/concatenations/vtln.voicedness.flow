<network>
  <in name="samples"/>
  <out name="out"/>

  <param name="start-time"/>
  <param name="end-time"/>
  <param name="speaker"/>

  <node name="vtln" filter="$(file)"
	ignore-unknown-parameters="true"
	start-time="$(start-time)" end-time="$(end-time)" speaker="$(speaker)"/>
  <link from="network:samples" to="vtln:samples"/>

  <node name="voicedness" filter="../voicedness/voicedness.postprocessing.flow"/>
  <link from="network:samples" to="voicedness:samples"/>
  <link from="vtln:out" to="voicedness:synchronization-target"/>

  <node name="concatenation" filter="generic-vector-f32-concat"
	check-same-length="true"/>
  <link from="vtln:out" to="concatenation:vtln"/>
  <link from="voicedness:out" to="concatenation:voicedness"/>
  <link from="concatenation" to="network:out"/>
</network>
