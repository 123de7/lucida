<network>
  <in name="in"/>
  <out name="out"/>

  <!-- mean normalization -->

  <node name="normalization" filter="signal-normalization"
	length="201" right="100" type="mean"/>
  <link from="network:in" to="normalization"/>

  <node name="scaling" filter="generic-vector-f32-multiplication"
	value="500"/>
  <link from="normalization" to="scaling"/>
  <link from="scaling" to="network:out"/>

</network>
