<network>
  <out name="features"/>

  <param name="input-file"/>
  <param name="start-time"/>
  <param name="end-time"/>
  <param name="track"/>

  <node name="samples" filter="samples.flow"
	input-file="$(input-file)" start-time="$(start-time)" end-time="$(end-time)" track="$(track)"/>

  <node name="mfcc" filter="mfcc.flow"
	nr-cepstrum-coefficients="16"/>
  <link from="samples:out" to="mfcc:samples"/>

  <node name="postprocessing" filter="postprocessing.flow"/>
  <link from="mfcc:features" to="postprocessing:in"/>
  <link from="postprocessing:out" to="network:features"/>
</network>
