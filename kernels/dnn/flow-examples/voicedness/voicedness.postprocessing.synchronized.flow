<network>
  <in name="samples"/>
  <out name="out"/>

  <!-- Synchronization to the window length used for creating the initial alignment -->
  <node name="initial-alignment-window" filter="signal-window" 
	type="hamming" shift=".01" length="0.025"/>
  <link from="network:samples" to="initial-alignment-window"/>

  <node name="voicedness-postprocessing" filter="voicedness.postprocessing.flow"/>
  <link from="network:samples" to="voicedness-postprocessing:samples"/>
  <link from="initial-alignment-window" to="voicedness-postprocessing:synchronization-target"/>
  <link from="voicedness-postprocessing:out" to="network:out"/>
</network>
