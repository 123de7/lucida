<network>
  <in name="samples"/>
  <in name="synchronization-target"/>
  <out name="out"/>

  <param name="window-length"/>

  <node name="voicedness" filter="voicedness-autocorrelation.flow"
	window-length="$(window-length)"/>
  <link from="network:samples" to="voicedness:samples"/>

  <node name="postprocessing" filter="postprocessing.flow"/>
  <link from="voicedness:out" to="postprocessing:in"/>

  <node name="synchronization" filter="signal-repeating-frame-prediction"/>
  <link from="network:synchronization-target" to="synchronization:target"/>
  <link from="postprocessing:out" to="synchronization"/>
  <link from="synchronization" to="network:out"/>
</network>
