<network>
  <in name="amplitude-spectrum"/>
  <out name="out"/>

  <node name="filterbank" filter="signal-filterbank"
	warping-function="mel" filter-width="268.258"/>
  <link from="network:amplitude-spectrum" to="filterbank"/>
  <link from="filterbank" to="network:out"/>
</network>
